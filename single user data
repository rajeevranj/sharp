<script type="text/javascript">
let Obj={
    name:"Sanjay",
    age:29
};
let obj_certain=JSON.stringify(Obj);
localStorage.setItem("obj",obj_certain);
let myobj_uncertain=JSON.parse(localStorage.getItem("obj"));
console.log(myobj_uncertain);
</script>
